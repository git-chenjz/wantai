@{
    ViewBag.Title = "个人中心";
    Layout = "~/Areas/Web/Views/Shared/_LayoutWeUI.cshtml";
}

<div class="weui_panel">
    <div class="weui_panel_bd">
        <div class="weui_media_box weui_media_small_appmsg">
            <div class="weui_cells weui_cells_access">
                <a class="weui_cell" href="@Url.Action("Profile")">
                    <div class="weui_cell_hd" style="width:30px;">
                        <img src="~/images/center01.png" width="20">
                    </div>
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>我的资料</p>
                    </div>
                    <span class="weui_cell_ft"></span>
                </a>

                <a class="weui_cell" href="@Url.Action("Inoculation")">
                    <div class="weui_cell_hd" style="width:30px;">
                        <img src="~/images/center02.png" width="20">
                    </div>
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>接种周期表</p>
                    </div>
                    <span class="weui_cell_ft"></span>
                </a>

                <a class="weui_cell" href="@Url.Action("Meeting")">
                    <div class="weui_cell_hd" style="width:30px;">
                        <img src="~/images/center03.png" width="20">
                    </div>
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>会议信息</p>
                    </div>
                    <span class="weui_cell_ft"></span>
                </a>

                <a class="weui_cell" href="@Url.Action("Message")">
                    <div class="weui_cell_hd" style="width:30px;">
                        <img src="~/images/center04.png" width="20">
                    </div>
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>消息提醒</p>
                    </div>
                    <span class="weui_cell_ft"></span>
                </a>

                @*<div class="weui_btn_area">
                    <a class="weui_btn weui_btn_plain_primary" href="@Url.Action("Index","Home")" id="showTooltips">返回</a>
                </div>*@
            </div>
        </div>
    </div>
</div>