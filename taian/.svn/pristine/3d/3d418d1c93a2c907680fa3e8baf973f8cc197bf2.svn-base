@charset "utf-8";
/* 创新疫苗详情页 */
.int-top {
	float: left;
	width: 100%;
	height: 3.41rem;
}
.int-top a h2 {
	overflow: hidden;
	width: 90%;
	height: 2.2rem;
	margin: 0.41rem auto 0;
	color: #f6931e;
	font-size: 0.8rem;
	line-height: 1.38em;
}
.int-wrap {
	overflow: hidden;
	width: 90%;
	height: 0.65rem;
	margin: 0 auto 0.5rem;
}
.wrap-text {
	overflow: hidden;
	float: left;
	width: 38%;
	height:0.65rem;
	text-align: left;
	color: #807e7e;
	font-size: 0.52rem;
	line-height: 0.52rem;
}
.wrap-demo {
	overflow: hidden;
	float: left;
	width: 22%;
	height: 0.55rem;
	color: #807e7e;
	font-size: 0.52rem;
	line-height: 0.52rem;
	text-align: right;
}
.wrap-demo i {
	float: left;
	width: 25%;
	height: 0.5rem;
	margin: 0 10% 0 10%;
	background-image: url(../images/link11.png);
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.wrap-demo span {
	float: left;
	color: #807e7e;
	font-size: 0.52rem;
	line-height: 0.52rem;
}
.int-content {
	width: 90%;
	margin: 0 auto;
	padding-bottom: 3rem;
}
.int-content p {
	color: #333;
	padding-top: 0.2rem; 
	font-size: 0.6rem;
	text-align: left;
	text-indent: 2em;
	line-height: 1rem;
}
.int-content img {
	display: block;
	width: 100%;
	height: 9rem;
	margin: 0.6rem 0;
}
.int-content span {
	float: left;
	width: 100%;
	color: #333;
	padding-top: 0.2rem;
	font-size: 0.55rem;
	text-align: left;
	line-height: 1rem;
}
.int-content a {
	float: left;
	width: 100%;
	color: #333;
	font-size: 0.55rem;
	text-align: left;
	line-height: 1rem;
}