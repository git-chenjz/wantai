.Uclearfix {
    zoom: 1;
    margin-bottom: 5px;
}

    .Uclearfix:after {
        content: " ";
        display: block;
        clear: both;
        height: 0;
        font-size: 0;
        visibility: hidden;
        line-height: 0;
    }

    .Uclearfix ul {
        margin: 0;
        padding: 0;
        list-style: none;
    }

        .Uclearfix ul li {
            float: left;
            width: 70px;
            text-align: center;
            cursor: pointer;
            padding: 5px 8px;
            margin-left:5px;
        }

            .Uclearfix ul li:hover {
                background-color: #00b38a;
                color: #fff;
            }


